{
  "name": "material-ui-dots",
  "description": "Just some animated dots for pagination.",
  "version": "2.0.3",
  "author": "Wertarbyte <kontakt@wertarbyte.com> (https://wertarbyte.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TeamWertarbyte/material-ui-dots/issues"
  },
  "homepage": "https://github.com/TeamWertarbyte/material-ui-dots#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TeamWertarbyte/material-ui-dots.git"
  },
  "keywords": [
    "react",
    "react-component",
    "material",
    "pagination",
    "dots",
    "animation"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "dist/material-ui-dots.umd.js",
  "module": "dist/material-ui-dots.js",
  "types": "dist/material-ui-dots.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/material-ui-dots.js",
      "require": "./dist/material-ui-dots.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -p ./tsconfig-build.json && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write 'src/**/*.{ts,tsx}' 'lib/**/*.{ts,tsx}'",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/material": "^6.1.0",
    "@mui/styles": "^6.1.0",
    "@types/node": "^20.14.6",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^4.0.1"
  },
  "peerDependencies": {
    "@mui/material": "^ 5.0.0 || ^6.0.0"
  }
}
